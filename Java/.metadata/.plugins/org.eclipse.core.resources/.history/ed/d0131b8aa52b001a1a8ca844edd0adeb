package pro1;

public class StrCheck {

	public StrCheck() {
		// TODO Auto-generated constructor stub
	}
	public String hasUpper(String data) {
		char [] c = data.toCharArray();
		for(int i=0;i<data.length();i++) {
			if(c[i]>='A' && c[i]<='Z') {
				return "Yes";
			}
			
		}
		
	}
	public String hasNumber(String data) {
		char [] c = data.toCharArray();
		
		for(int i=0;i<data.length();i++) {
			if(c[i]>=0 && c[i]<=9) {
				return "YES";
			}
		}
	}
	public boolean CapsOnly(String data) {
		char [] c = data.toCharArray();
		int ctr=0;
		for(int i=0;i<data.length();i++) {
			if(c[i]>='A' && c[i]<='Z')
				ctr++;
		}
		if(ctr==data.length()) {
			return true;
		}
		
	}
	public int countCaps(String data) {
		char [] c = data.toCharArray();
		int ctr=0;
		for(int i=0;i<data.length();i++) {
			if(c[i]>='A' && c[i]<='Z')
				ctr++;
		}
		return ctr;
	}
	public boolean isEqual(char c1,char c2) {
		
		if() {
			return true; 
		}
		return false;
	}
	public boolean isEqual(String str1,String str2) {
		
		if(str1.equalsIgnoreCase(str2)) {
			return true; 
		}
		return false;
	}
	public static void main(String[] args) {
		StrCheck sc = new StrCheck();
		sc.hasUpper("Hello");
	}
}
