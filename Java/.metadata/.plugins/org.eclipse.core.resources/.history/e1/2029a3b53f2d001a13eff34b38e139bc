		India i = new India();
		System.out.println(i.numberFormat("+91-9492728533"));
	}
}

class India extends Country implements PhoneNumber{
	
	public boolean numberFormat(String number) {
		
		char [] c = number.toCharArray();
		int count = number.length();
		if(number.length()==14) {
			if(c[0]=='+' && c[1]=='9' && c[2]=='1' && c[3]=='-'){
				
				for(int i =4; i<count-4;i++) {
					if(c[i]>='0'|| c[i]<='9' )
						
						return true;
				}
			}
		}
		return false;
	}
}

class Brazil extends Country implements PhoneNumber{
	
public boolean numberFormat(String number) {
		
		char [] c = number.toCharArray();
		int count = number.length();
		if(number.length()==17) {
			if(c[0]=='+' && c[1]=='5' && c[2]=='5' && c[3]==' '){
				
				for(int i =4; i<count;i++) {
					if(c[12]=='-') {
						continue;
					}
					if(c[i]>='0'|| c[i]<='9' )
						
						return true;
				}
			}
		}
		return false;
	}
}

