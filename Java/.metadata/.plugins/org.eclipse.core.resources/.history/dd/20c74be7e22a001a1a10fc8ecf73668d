package pro1;

public class Car {

	double price;
	String color;
	String model;
	String brand;
	
	public String details() {
		String details;
		
		details = "Brand "+brand+"Model "+model+"Color "+color+"Price "+price;
		
		return details;
	}
	public Car(String brand,String model,String color,double price) {
		// TODO Auto-generated constructor stub
	this.brand=brand;
	this.color=color;
	this.model=model;
	this.price=price;
	
		
	}


}
