package pro1;

public class Test {
	static int num =0;
	String brand;
	String model;
	String color;
	double price;
	
	public String details() {
		String det = "Brand "+brand+"\nModel "+model+"\nColor "+color+"\nPrice "+price;
		return det;
	}
	public Test(String brand,String model,String color,double price) {
		
		
		// TODO Auto-generated constructor stub
		this.brand=brand;
		this.model=model;
		this.color=color;
		this.price=price;
	}

	public static void main(String [] args) {
		Test t = new Test();
		
		//t.details("Volvo","s60","red",4500000);
		
		System.out.println(t.details("Volvo","s60","red",4500000));
	}

}
