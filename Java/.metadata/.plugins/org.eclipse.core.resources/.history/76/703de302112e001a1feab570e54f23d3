package pro1;

import java.io.File; 
import java.util.Scanner; 
public class Scan 
{ 
  public static void main(String[] args) throws Exception 
  { 
    File file = 
      new File("C:\\Users\\admin\\Documents\\GitHub\\DelNex4D\\Java\\pro1\\src\\pro1\\data.txt"); 
    Scanner sc = new Scanner(file); 
  
    while (sc.hasNextLine()) {
//      System.out.println(sc.nextLine()); 
    	String a[] = (sc.nextLine()+"\n").split(",");
    	System.out.print("\t");
    	for(String str:a) {
//    		System.out.print("\t");
    		System.out.print(str+"\t");
    		
    	}
    	
  } 
  }
} 