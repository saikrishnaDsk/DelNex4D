package pro1;

import java.io.File; 
import java.util.Scanner; 
public class Scan 
{ 
  public static void main(String[] args) throws Exception 
  { 
    File file = 
      new File("/data.txt"); 
    Scanner sc = new Scanner(file); 
  
    while (sc.hasNextLine()) 
      System.out.println(sc.nextLine()); 
  } 
} 